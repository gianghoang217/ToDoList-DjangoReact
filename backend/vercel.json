{
  "framework": null,
  "builds": [
    {
      "src": "backend/todo_project/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize":  "15mb", "runtime":  "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/todo_project/wsgi.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
      }
    }
  ]
}